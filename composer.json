{
    "name": "ridvanbaluyos/semaphore",
    "version": "v0.1",
    "description": "A Laravel 4.2 Package for the Sempahore SMS API.",
    "type": "laravel-package",
    "license": "MIT",
    "keywords": [
        "sms",
        "philippines",
        "semaphore",
        "laravel",
        "api"
    ],
    "authors": [
